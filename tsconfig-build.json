{
  "compilerOptions": {
    "lib": ["es5", "es6", "DOM"],
    "target": "es6",
    "module": "es2015",
    "newLine": "lf",
    "moduleResolution": "node",
    "declaration": true,
    "strict": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
  },
  "include": ["src/*.ts"]
}
